pipeline
 {
  agent any

  stages 
        {
         stage('install dependencies')
          {
           steps 
             { 
               sh 'npm install'
              }
             }
          stage('Build')
           {
            steps
             {
              sh 'echo "building application...."'
              }
            }
          stage('Deploy Application')
           {
            steps
             {
              sh 'echo "deploying application...."'
              }
             }
          }
  }
